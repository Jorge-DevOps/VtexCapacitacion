{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },

  //---Layouts
  "footer-layout.desktop": {
    "props": {
      "blockClass": "layout-desktop"
    },
    "children": ["flex-layout.row#footer-desktop"]
  },
  "footer-layout.mobile": {
    "props": {
      "blockClass": "layout-mobile"
    },
    "children": ["flex-layout.row#footer-mobile"]
  },

  "flex-layout.row#footer-mobile": {
    "props": {
      "blockClass": "layout-rowFooter"
    },
    "children": [
      "flex-layout.col#article1",
      "flex-layout.col#article2",
      "flex-layout.col#article3",
      "social-networks#mobile",
      "accepted-payment-methods#mobile",
      "image#ePartner",
      "image#vtex"
    ]
  },

  "flex-layout.row#footer-desktop": {
    "props": {
      "blockClass": "layout-rowFooter"
    },
    "children": [
      "flex-layout.col#article1",
      "flex-layout.col#article2",
      "flex-layout.col#article3",
      "flex-layout.col#article4"
    ]
  },

  "flex-layout.col#article1": {
    "props": {
      "blockClass": "article-list"
    },
    "children": [
      "rich-text#title1",
      "rich-text#subtitle1.1",
      "rich-text#subtitle1.2",
      "rich-text#subtitle1.3"
    ]
  },

  "flex-layout.col#article2": {
    "props": {
      "blockClass": "article-list"
    },
    "children": [
      "rich-text#title2",
      "rich-text#subtitle2.1",
      "rich-text#subtitle2.2",
      "rich-text#subtitle2.3"
    ]
  },

  "flex-layout.col#article3": {
    "props": {
      "blockClass": "article-list"
    },
    "children": [
      "rich-text#title3",
      "rich-text#subtitle3.1",
      "rich-text#subtitle3.2",
      "rich-text#subtitle3.3"
    ]
  },

  "flex-layout.col#article4": {
    "props": {
      "blockClass": "article-list"
    },
    "children": [
      "flex-layout.row#rowPayment",
      "flex-layout.row#rowSocial",
      "flex-layout.row#rowPowerBy"
    ]
  },

  "flex-layout.row#rowSocial": {
    "props": {
      "blockClass": "rowSocial"
    },
    "children": ["social-networks#desktop"]
  },
  "flex-layout.row#rowPayment": {
    "props": {
      "blockClass": "rowPayment"
    },
    "children": ["accepted-payment-methods#desktop"]
  },
  "flex-layout.row#rowPowerBy": {
    "props": {
      "blockClass": "rowPowerBy"
    },
    "children": ["image#vtex", "image#ePartner"]
  },

